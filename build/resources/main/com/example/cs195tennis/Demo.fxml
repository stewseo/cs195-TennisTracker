<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.geometry.Insets?>
<?import io.github.palexdev.materialfx.font.MFXFontIcon?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import io.github.palexdev.materialfx.controls.MFXScrollPane?>
<?import javafx.scene.layout.StackPane?>

<AnchorPane fx:id="rootPane" prefHeight="1020.0" prefWidth="1380.0" styleClass="rootPane" stylesheets="@Demo.css"
            xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.cs195tennis.controller.DemoController">
    <HBox fx:id="windowHeader" alignment="CENTER_RIGHT" layoutY="6.0" prefHeight="30.0" prefWidth="1480.0"
          spacing="10.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <padding>
            <Insets right="10.0"/>
        </padding>
        <MFXFontIcon fx:id="alwaysOnTopIcon" description="mfx-circle" size="15.0" styleClass="always-on-top-icon"/>
        <MFXFontIcon fx:id="minimizeIcon" description="mfx-circle" size="15.0" styleClass="minimize-icon"/>
        <MFXFontIcon fx:id="closeIcon" description="mfx-circle" size="15.0" styleClass="close-icon"/>
    </HBox>
    <VBox layoutX="200.0" layoutY="51.0" prefWidth="200.0" styleClass="sidebar" AnchorPane.bottomAnchor="0.0"
          AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
        <padding>
            <Insets bottom="10.0" left="10.0" top="60.0"/>
        </padding>
        <Label graphicTextGap="15.0" styleClass="header" text="CS195 Tennis Analytics">
            <graphic>
                <MFXFontIcon description="mfx-logo" size="52.0"/>
            </graphic>
        </Label>
        <Label styleClass="menu-label" text="Tables">
            <VBox.margin>
                <Insets left="5.0" top="40.0"/>
            </VBox.margin>
        </Label>
        <MFXScrollPane fx:id="scrollPane" fitToWidth="true" hbarPolicy="NEVER" VBox.vgrow="ALWAYS">
            <VBox.margin>
                <Insets bottom="10.0" left="5.0" top="15.0"/>
            </VBox.margin>
            <padding>
                <Insets right="10.0"/>
            </padding>
            <VBox fx:id="navBar" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0"
                  styleClass="navbar"/>
        </MFXScrollPane>
    </VBox>
    <StackPane fx:id="contentPane" layoutX="200.0" layoutY="52.0" prefHeight="350.0" prefWidth="300.0"
               AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="200.0" AnchorPane.rightAnchor="10.0"
               AnchorPane.topAnchor="52.0"/>
</AnchorPane>